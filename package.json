{
  "name": "vue-grid-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "buid:bundle": "vue-cli-service build --target lib --name vue-grid-builder ./src/components/grid",
    "lint": "vue-cli-service lint",
    "lint:fix": "prettier-eslint --write \"src/**/*.{js,vue,css}\"",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "postcss-utilities": "^0.8.0",
    "prettier-eslint": "^8.8.2",
    "vue": "^2.5.17",
    "vue-drag-select": "^0.1.5",
    "vue-grid-layout": "^2.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-e2e-nightwatch": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-jest": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "lint-staged": "^7.2.2",
    "postcss-preset-env": "^6.2.0",
    "prettier-eslint-cli": "^4.7.1",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write \"src/**/*.js\"",
      "git add"
    ],
    "*.vue": [
      "prettier-eslint --write \"src/**/*.vue\"",
      "git add"
    ],
    "*.css": [
      "prettier-eslint --write \"src/**/*.css\"",
      "git add"
    ]
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "engines": {
    "node": "^10.0.0",
    "yarn": "^1.9.0"
  }
}
